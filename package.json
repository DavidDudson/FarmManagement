{
  "name": "farmFINANZ",
  "version": "0.1.1",
  "description": "Farm Knowledge assessor for the Farm Management Institute, Massey University",
  "main": "./src/start.js",
  "scripts": {
    "test": "karma start",
    "clean": "rm -r dist/*",
    "mkdir": "mkdir -p dist/public",
    "copy:index": "cp src/client/index.html dist/public/index.html",
    "copy:iframe": "cp src/client/iframe.css dist/public/iframe.css",
    "copy": "npm run copy:index && npm run copy:iframe",
    "quicktest": "open ./dist/public/test.html",
    "start": "node ./src/start.js",
    "webpack": "webpack --config=\"src/webpack.conf.js\" --progress",
    "deploy": "git push heroku master",
    "babel-node": "babel-node --stage 0 --ignore='node_modules|dist'",
    "develop": "nodemon --watch src src/start.js -r npm run babel-node -- ./src/server/",
    "webpackDevServer": "webpack-dev-server --config src/webpack.conf.js --content-base dist/public --hot --inline",
    "develop2": "(npm run clean || ! npm run clean) && npm run mkdir && npm run copy && npm run webpackDevServer & open http://localhost:8080"
  },
  "repository": "https://github.com/DavidDudson/FarmManagement.git",
  "author": "David J. Dudson, Anthony Crowcroft",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.4.7",
    "angular-animate": "^1.4.7",
    "angular-aria": "^1.4.7",
    "angular-carousel": "^1.0.1",
    "angular-data-table": "^0.4.8",
    "angular-material": "^0.11.4",
    "angular-material-data-table": "^0.8.11",
    "angular-touch": "^1.4.8",
    "angular-ui-router": "^0.2.15",
    "babel-loader": "^6.2.0",
    "compass-mixins": "^0.12.7",
    "css-loader": "^0.23.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "glob": "^6.0.3",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.4",
    "lodash": "^3.10.1",
    "material-design-icons-iconfont": "^2.0.3",
    "mathjs": "^2.5.0",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "resolve-url-loader": "^1.4.2",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ui-router": "^0.2.15",
    "url-loader": "^0.5.6",
    "walk": "^2.3.9",
    "webpack": "^1.12.6",
    "webpack-notifier": "^1.2.1"
  },
  "engines": {
    "node": "5.1"
  },
  "babel": {
    "ignore": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "comments": false,
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "expect.js": "^0.3.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-mocha": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-loader": "^0.7.1",
    "phantomjs": "^1.9.19"
  }
}
